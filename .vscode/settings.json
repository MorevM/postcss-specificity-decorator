{
	"prettier.enable": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"source.fixAll.stylelint": "explicit"
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"vue",
		"markdown",
		"yaml",
		"json"
	],
	"css.validate": false,
	"less.validate": false,
	"scss.validate": false
}
